<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66c3b6">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="用户名："
        android:id="@+id/textView3"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="62dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="40dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:id="@+id/et_user"
        android:layout_alignBottom="@+id/textView3"
        android:layout_toEndOf="@+id/textView3"
        android:inputType="text"
        android:layout_marginBottom="-5dp"
        android:paddingBottom="6dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="检测"
        android:id="@+id/bt_check"
        android:layout_alignBottom="@+id/et_user"
        android:layout_toEndOf="@+id/et_user"
        android:layout_alignParentStart="false"
        android:background="#6ad5af"
        android:layout_alignTop="@+id/et_user" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="密  码："
        android:id="@+id/textView4"
        android:layout_below="@+id/textView3"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/et_psd"
        android:layout_alignBottom="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:layout_alignLeft="@+id/et_user"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="重复密码："
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/et_psdt"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toEndOf="@+id/textView3"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="公司："
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/et_company"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toEndOf="@+id/textView5"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="手机："
        android:id="@+id/textView7"
        android:layout_below="@+id/textView6"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/et_phone"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/textView6"
        android:inputType="text"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="邮箱："
        android:id="@+id/textView8"
        android:layout_below="@+id/textView7"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/et_email"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toEndOf="@+id/textView7"
        android:layout_marginBottom="-5dp"
        android:paddingBottom="6dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="地址："
        android:id="@+id/textView9"
        android:layout_below="@+id/textView8"
        android:layout_alignEnd="@+id/textView3"
        android:layout_marginTop="20dp"
        android:textColor="#020202" />

    <EditText
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:id="@+id/et_sheng"
        android:layout_alignBottom="@+id/textView9"
        android:layout_toEndOf="@+id/textView3"
        android:inputType="text"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="省"
        android:id="@+id/textView10"
        android:layout_toEndOf="@+id/et_sheng"
        android:layout_alignBottom="@+id/et_sheng"
        android:textColor="#020202" />

    <EditText
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:id="@+id/et_shi"
        android:layout_alignBottom="@+id/textView10"
        android:layout_toEndOf="@+id/textView10"
        android:inputType="text"
        android:paddingBottom="6dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="市"
        android:id="@+id/textView11"
        android:layout_alignBottom="@+id/et_shi"
        android:layout_toEndOf="@+id/et_shi"
        android:textColor="#020202" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/et_add"
        android:layout_below="@+id/et_sheng"
        android:layout_alignEnd="@+id/et_email"
        android:layout_toEndOf="@+id/textView9"
        android:inputType="text"
        android:paddingBottom="6dp"
        android:layout_marginBottom="-5dp"
        android:layout_marginTop="10dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="重置"
        android:id="@+id/bt_clear"
        android:layout_below="@+id/et_add"
        android:layout_alignStart="@+id/textView4"
        android:layout_marginTop="48dp"
        android:textSize="20dp"
        android:background="#4a83a4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="注册"
        android:id="@+id/bt_regist"
        android:layout_alignBottom="@+id/bt_clear"
        android:layout_toEndOf="@+id/textView10"
        android:textSize="20dp"
        android:background="#5b9455" />

    <Button
        android:id="@+id/bt_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/register_login"
        android:clickable="true"
        android:gravity="left|center"
        android:paddingLeft="18dp"
        android:paddingRight="0dp"
        android:text="&lt;-返回"
        android:textColor="#ffffffff"
        android:textSize="18dp"
        android:onClick="back"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/bt_check" />

</RelativeLayout>